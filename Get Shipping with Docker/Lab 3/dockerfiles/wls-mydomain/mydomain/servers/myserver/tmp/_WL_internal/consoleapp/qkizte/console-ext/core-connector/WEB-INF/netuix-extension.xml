<?xml version="1.0" encoding="UTF-8"?>
<weblogic-portal-extension
  xmlns="http://www.bea.com/servers/portal/weblogic-portal/8.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.bea.com/servers/portal/weblogic-portal/8.0 netuix-extension-1_0_0.xsd">

  <provider-info>
    <title>Resource Adapter Pages</title>
    <version>10.3</version>
    <description>WebLogic Server console pages for Resource Adapter support</description>
    <author>Oracle</author>
    <support-url>http://www.oracle.com</support-url>
  </provider-info>

  <portal-file>/console.portal</portal-file>

  <book-extension>
    <book-location>
      <parent-label-location label="AppDeploymentsMonitoringPage"/>
      <book-insertion-point page-label="AppDeploymentsMonitorWebAppPage" action="append"/>
    </book-location>
	<page-content content-uri="/PortalConfig/app/core-connector.book" />
  </book-extension>
  
</weblogic-portal-extension>
