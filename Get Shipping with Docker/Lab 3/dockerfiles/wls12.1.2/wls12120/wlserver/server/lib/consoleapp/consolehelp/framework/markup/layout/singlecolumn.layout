<?xml version="1.0" encoding="UTF-8"?>
<netuix:markupDefinition xmlns:netuix="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0 markup-netuix-1_0_0.xsd">
    <netuix:locale language="en"/>
    <netuix:markup>
        <netuix:gridLayout
                           title="Single Column Layout" description="This layout uses the gridLayout control to create one column in which placeables flow vertically. onecolumnflow.layout is simpler and outputs less markup, and therefore is generally preferred to this layout. It is being retained for backward compatibility."
                           columns="1" htmlLayoutUri="/framework/markup/layout/singlecolumn.html.txt" iconUri="/framework/markup/layout/singlecolumn.gif"
                           markupType="Layout" markupName="singleColumnLayout">
            <netuix:placeholder title="single" description="The only placeholder in this layout."
                                flow="vertical" width="100%"
                                markupType="Placeholder" markupName="singleColumn_columnOne">
            </netuix:placeholder>
        </netuix:gridLayout>
    </netuix:markup>
</netuix:markupDefinition>
