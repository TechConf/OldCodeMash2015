<?xml version="1.0" encoding="UTF-8" ?>
<product-family name="wls" >

  <!-- The WebLogic Server Administration Console application -->
  <application name="consoleapp" >
  
    <!-- The context root is configurable, but the default is /console -->
    <context-root>/console</context-root>
    
  </application>
  
  <!-- The WebLogic Server Administration Console Help application -->
  <application name="consolehelp" >
  
    <context-root>/consolehelp</context-root>
    
  </application>

</product-family>