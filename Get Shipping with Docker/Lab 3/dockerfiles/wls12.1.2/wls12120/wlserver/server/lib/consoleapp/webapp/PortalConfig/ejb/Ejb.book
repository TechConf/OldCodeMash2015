<?xml version="1.0" encoding="UTF-8"?>
<portal:root xmlns:netuix="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0"
             xmlns:html="http://www.w3.org/1999/xhtml-netuix-modified/1.0.0"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:portal="http://www.bea.com/servers/netuix/xsd/portal/support/1.0.0"
             xsi:schemaLocation="http://www.bea.com/servers/netuix/xsd/portal/support/1.0.0 portal-support-1_0_0.xsd">
<!--
    Definition file for the EJB pages in the j2ee subsystem
    of the WebLogic Server administration console.

    There are several main branches in this book:

     - EJBApplicationBook is used for a standalone EJB
     - EJBModuleBook is used for an EJB module within an app
     - EJBComponentBook is used for an individual bean

    Copyright (c) 2004,2011, Oracle and/or its affiliates. All rights reserved.
-->
  <!-- Definition for the EJBApplicationBook configuration pages -->
  <netuix:book definitionLabel="EJBApplication" title="book.title" markupName="book"
               presentationClass="invisible">
    <netuix:content>
      <netuix:book definitionLabel="EJBApplicationBook" title="book.title" markupName="book"
                   backingFile="com.bea.console.utils.AppDeploymentBackingFile"
                   threadsafe="true"
                   presentationClass="wlsc-frame">
        <netuix:titlebar/>
        <netuix:content>
          <netuix:page markupName="page" definitionLabel="EJBApplicationPages" title="not.used"
                       hidden="true">
            <netuix:content>
              <netuix:layout type="no" markupName="NoLayout">
                <netuix:placeholder usingFlow="false" markupName="ph1">
                  <netuix:book markupName="book" definitionLabel="EJBApplicationTabsBook" title="AppBook">
                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                    <netuix:content>
                      <!-- Definition for the Overview tab -->
                      <netuix:page markupName="page" definitionLabel="EJBApplicationOverviewPage"
                                   presentationClass="page-content"
                                   title="tab.overview.label">
                        <netuix:meta name="helpid" content="J2EE.ejb.ejboverview.title;j2ee"/>
                        <netuix:meta name="breadcrumb-context" content="handle"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portlet definitionLabel="EJBApplicationOverviewPortlet">
                                <netuix:strutsContent module="/j2ee" action="EJBApplicationOverview"
                                                      refreshAction="EJBApplicationOverview"/>
                              </netuix:portlet>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the Deployment Plan tab -->
                      <netuix:book markupName="book" definitionLabel="EJBApplicationDeploymentPlanPage"
                                   title="tab.deploymentplan.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for the Tuning Parameters tab -->
                          <netuix:page markupName="page" definitionLabel="EJBApplicationTuningParameters"
                                       presentationClass="page-content"
                                       title="tab.tunables.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationtunables.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <!-- Header text shared by all Tuning Parameter pages -->
                                  <netuix:portlet definitionLabel="EJBApplicationTunablesHeaderPortlet">
                                    <netuix:strutsContent module="/j2ee" action="ApplicationTunablesHeader"
                                                          refreshAction="ApplicationTunablesHeader"/>
                                  </netuix:portlet>
                                  <netuix:book presentationClass="invisible" title="title" markupName="book"
                                               definitionLabel="EJBApplicationTuningParametersBook">
                                    <netuix:content>
                                      <netuix:page markupName="page" definitionLabel="EJBApplicationTunablesContents"
                                                   title="tab.deploymentplan.label">
                                        <netuix:content>
                                          <netuix:flowLayout orientation="horizontal" markupType="Layout" markupName="twoColumnLayout">
                                            <!-- Navigation tree shared by all Tuning Parameter pages -->
                                            <netuix:placeholder title="left" presentationId="tuning-nav-col" markupName="twoColumn_left"
                                                                usingFlow="false">
                                              <netuix:portlet definitionLabel="EJBApplicationTunablesTreePortlet">
                                                <netuix:strutsContent module="/j2ee" action="ApplicationTunablesTree"
                                                                      refreshAction="ApplicationTunablesTree"/>
                                              </netuix:portlet>
                                            </netuix:placeholder>
                                            <!-- Content pane for Tuning Parameter pages -->
                                            <netuix:placeholder title="right" presentationId="tuning-content-col" markupName="twoColumn_right"
                                                                usingFlow="false">
                                              <netuix:book definitionLabel="EJBApplicationTunablesContentsBook" title="book.title"
                                                           markupName="book">
                                                <netuix:content>
                                                  <!-- The default, single-tab configuration page -->
                                                  <netuix:book markupName="book" definitionLabel="EJBApplicationTunablesContentsDefaultPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="EJBApplicationTunablesContentsDefaultPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portlet definitionLabel="EJBApplicationTunablesContentsDefaultPortlet">
                                                                <netuix:strutsContent module="/j2ee" action="ApplicationTunablesContentsDefault"
                                                                                      refreshAction="ApplicationTunablesContentsDefault"/>
                                                              </netuix:portlet>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The single tunable tab for an EJB -->
                                                  <netuix:book markupName="book" definitionLabel="EJBTunablesPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="EJBTunablesPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.config.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="EJBTunablesPortlet"
                                                                                      contentUri="/PortalConfig/ejb/EJBTunables.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The single tunable tab for a web service -->
                                                  <netuix:book markupName="book" definitionLabel="EJBWebServiceTunablesPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="EJBWebServiceTunablesGeneralPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.general.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet" instanceLabel="EJBWebServiceTunablesGeneralPortlet"
                                                                                      contentUri="/PortalConfig/webservice/WebServiceTunablesGeneral.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The annotation editing page -->
                                                  <netuix:book markupName="book" definitionLabel="EJBApplicationTunablesEditAnnotationPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="EJBApplicationTunablesEditAnnotationPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="EJBApplicationTunablesEditAnnotationPortlet"
                                                                                      contentUri="/PortalConfig/app/EditAnnotation.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                </netuix:content>
                                              </netuix:book>
                                            </netuix:placeholder>
                                          </netuix:flowLayout>
                                        </netuix:content>
                                      </netuix:page>
                                    </netuix:content>
                                  </netuix:book>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Resource Dependencies tab -->
                          <netuix:page markupName="page" definitionLabel="EJBApplicationBindablesPages"
                                       presentationClass="page-content"
                                       title="tab.bindables.label">
                            <netuix:meta name="helpid" content="J2EE.app.applicationdescriptors.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <!-- Header text shared by all Dependencies pages -->
                                  <netuix:portlet definitionLabel="EJBApplicationBindablesPortlet">
                                    <netuix:strutsContent module="/j2ee" action="ApplicationBindablesHeader"
                                                          refreshAction="ApplicationBindablesHeader"/>
                                  </netuix:portlet>
                                  <netuix:book presentationClass="invisible" title="title" markupName="book"
                                               definitionLabel="EJBApplicationBindablesContentsBooks">
                                    <netuix:content>
                                      <netuix:page markupName="page" definitionLabel="EJBApplicationBindablesContents"
                                                   title="tab.deploymentplan.label">
                                        <netuix:content>
                                          <netuix:flowLayout orientation="horizontal" markupType="Layout" markupName="twoColumnLayout">
                                            <!-- Navigation tree shared by all Dependencies pages -->
                                            <netuix:placeholder title="left" presentationId="tuning-nav-col" markupName="twoColumn_left"
                                                                usingFlow="false">
                                              <netuix:portlet definitionLabel="EJBApplicationBindablesTreePortlet">
                                                <netuix:strutsContent module="/j2ee" action="ApplicationBindablesTree"
                                                                      refreshAction="ApplicationBindablesTree"/>
                                              </netuix:portlet>
                                            </netuix:placeholder>
                                            <!-- Content pane for Dependencies pages -->
                                            <netuix:placeholder title="right" presentationId="tuning-content-col" markupName="twoColumn_right"
                                                                usingFlow="false">
                                              <netuix:book definitionLabel="EJBApplicationBindablesContentsBook" title="book.title"
                                                           markupName="book">
                                                <netuix:content>
                                                  <!-- The default, single-tab resource configuration page -->
                                                  <netuix:book markupName="book" definitionLabel="EJBApplicationBindablesContentsPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="EJBApplicationBindablesContentsPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portlet definitionLabel="EJBApplicationBindablesContentsPortlet">
                                                                <netuix:strutsContent module="/j2ee" action="ApplicationBindablesContents"
                                                                                      refreshAction="ApplicationBindablesContents"/>
                                                              </netuix:portlet>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                  <!-- The annotation editing page -->
                                                  <netuix:book markupName="book" definitionLabel="EJBApplicationBindablesEditAnnotationPages"
                                                               title="tab.deploymentplan.label">
                                                    <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu"/>
                                                    <netuix:content>
                                                      <netuix:page markupName="page" definitionLabel="EJBApplicationBindablesEditAnnotationPage"
                                                                   presentationClass="page-content"
                                                                   title="tab.configuration.label">
                                                        <netuix:content>
                                                          <netuix:layout type="no" markupName="NoLayout">
                                                            <netuix:placeholder usingFlow="false" markupName="ph1">
                                                              <netuix:portletInstance markupType="Portlet"
                                                                                      instanceLabel="EJBApplicationBindablesEditAnnotationPortlet"
                                                                                      contentUri="/PortalConfig/app/EditAnnotation.portlet"/>
                                                            </netuix:placeholder>
                                                          </netuix:layout>
                                                        </netuix:content>
                                                      </netuix:page>
                                                    </netuix:content>
                                                  </netuix:book>
                                                </netuix:content>
                                              </netuix:book>
                                            </netuix:placeholder>
                                          </netuix:flowLayout>
                                        </netuix:content>
                                      </netuix:page>
                                    </netuix:content>
                                  </netuix:book>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the Configuration tab -->
                      <netuix:book markupName="book" definitionLabel="EJBApplicationConfigurationPage"
                                   title="tab.configuration.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- definition for the Instrumentation subtab-->
                          <netuix:page markupName="page"
                                       definitionLabel="EJBApplicationConfigurationInstrumentationPage"
                                       presentationClass="page-content"
                                       title="tab.diagnostics.instrumentation.label">
                            <netuix:meta name="helpid"
                                         content="Diagnostics.diagnostics.moduleconfiginstrumentation.title;diagnostics"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet"
                                                          instanceLabel="EJBApplicationConfigurationInstrumentationPortlet"
                                                          contentUri="/PortalConfig/diagnostics/module/ModuleConfigInstrumentation.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the Persistence tab -->
                          <netuix:page markupName="page" definitionLabel="EJBApplicationPersistenceUnitTablePage"
                                       presentationClass="page-content"
                                       title="tab.persistenceunit.table.label">
                            <netuix:meta name="helpid" content="J2EE.persistenceunit.table.title;j2ee"/>
                            <netuix:meta name="breadcrumb-context" content="handle"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portletInstance markupType="Portlet" instanceLabel="EJBApplicationPersistenceUnitTablePortlet"
                                                          contentUri="/PortalConfig/persistence/PersistenceUnitTable.portlet"/>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the security tab -->
                      <netuix:book markupName="book" definitionLabel="EJBApplicationSecurityPage"
                                   title="tab.security.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <netuix:book markupName="book" definitionLabel="EJBApplicationApplicationSecurityPage"
                                       title="tab.applicationscope.label">
                            <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level2"/>
                            <netuix:content>
                              <!-- definition for ejb application roles table page -->
                              <netuix:page markupName="page" definitionLabel="EJBApplicationSecurityRolesTablePage"
                                           presentationClass="page-content"
                                           title="tab.roles.label">
                                <netuix:meta name="helpid" content="J2EE.ejb.ejbapplicationrole.table.title;j2ee"/>
                                <netuix:meta name="breadcrumb" content="tab.roles.label;key"/>
                                <netuix:content>
                                  <netuix:layout type="no" markupName="NoLayout">
                                    <netuix:placeholder usingFlow="false" markupName="ph1">
                                      <netuix:portlet definitionLabel="EJBApplicationSecurityRolesTablePortlet">
                                        <netuix:strutsContent module="/j2ee" action="EJBApplicationSecurityRolesTable"
                                                              refreshAction="EJBApplicationSecurityRolesTable"/>
                                      </netuix:portlet>
                                    </netuix:placeholder>
                                  </netuix:layout>
                                </netuix:content>
                              </netuix:page>
                              <!-- definition for ejb application policies page -->
                              <netuix:page markupName="page" definitionLabel="EJBApplicationSecurityPoliciesPage"
                                           presentationClass="page-content"
                                           title="tab.policies.label">
                                <netuix:meta name="helpid" content="J2EE.ejb.application.policies.page.title;j2ee"/>
                                <netuix:meta name="breadcrumb" content="tab.policies.label;key"/>
                                <netuix:content>
                                  <netuix:layout type="no" markupName="NoLayout">
                                    <netuix:placeholder usingFlow="false" markupName="ph1">
                                      <netuix:portlet definitionLabel="EJBApplicationSecurityPoliciesPortlet">
                                        <netuix:pageflowContent contentUri="/com/bea/console/actions/ejb/policies/edit/app/EditEJBAppPolicies.jpf"
                                                                action="begin"
                                                                refreshAction="begin"/>
                                      </netuix:portlet>
                                    </netuix:placeholder>
                                  </netuix:layout>
                                </netuix:content>
                              </netuix:page>
                            </netuix:content>
                          </netuix:book>
                          <netuix:book markupName="book" definitionLabel="EJBApplicationModuleSecurityPage"
                                       title="tab.modulescope.label">
                            <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level2"/>
                            <netuix:content>
                              <!-- definition for ejb application roles table page -->
                              <netuix:page markupName="page" definitionLabel="EJBApplicationModuleRolesTablePage"
                                           presentationClass="page-content"
                                           title="tab.roles.label">
                                <netuix:meta name="helpid" content="J2EE.ejb.ejbapplicationmodulerole.table.title;j2ee"/>
                                <netuix:meta name="breadcrumb" content="tab.roles.label;key"/>
                                <netuix:content>
                                  <netuix:layout type="no" markupName="NoLayout">
                                    <netuix:placeholder usingFlow="false" markupName="ph1">
                                      <netuix:portlet definitionLabel="EJBApplicationModuleRolesTablePortlet">
                                        <netuix:strutsContent module="/j2ee" action="EJBApplicationModuleRolesTable"
                                                              refreshAction="EJBApplicationModuleRolesTable"/>
                                      </netuix:portlet>
                                    </netuix:placeholder>
                                  </netuix:layout>
                                </netuix:content>
                              </netuix:page>
                              <!-- definition for ejb module inside standalone application policies page -->
                              <netuix:page markupName="page" definitionLabel="EJBApplicationModulePoliciesPage"
                                           presentationClass="page-content"
                                           title="tab.policies.label">
                                <netuix:meta name="helpid" content="J2EE.ejb.applicationmodule.policies.page.title;j2ee"/>
                                <netuix:meta name="breadcrumb" content="tab.policies.label;key"/>
                                <netuix:content>
                                  <netuix:layout type="no" markupName="NoLayout">
                                    <netuix:placeholder usingFlow="false" markupName="ph1">
                                      <netuix:portlet definitionLabel="EJBApplicationModulePoliciesPortlet">
                                        <netuix:pageflowContent contentUri="/com/bea/console/actions/ejb/policies/edit/appmodule/EditEJBAppModulePolicies.jpf"
                                                                action="begin"
                                                                refreshAction="begin"/>
                                      </netuix:portlet>
                                    </netuix:placeholder>
                                  </netuix:layout>
                                </netuix:content>
                              </netuix:page>
                            </netuix:content>
                          </netuix:book>
                        </netuix:content>
                      </netuix:book>
                      <!-- Definition for the targets tab -->
                      <netuix:page markupName="page" definitionLabel="EJBApplicationTargetsPage"
                                   presentationClass="page-content"
                                   title="tab.targets.label">
                        <netuix:meta name="helpid" content="J2EE.ejb.ejbtargets.title;j2ee"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portlet definitionLabel="EJBApplicationTargetsPortlet">
                                <netuix:strutsContent module="/j2ee" action="EJBTargets" refreshAction="EJBTargets"/>
                              </netuix:portlet>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the control tab -->
                      <netuix:page markupName="page" definitionLabel="EJBApplicationControlPage"
                                   presentationClass="page-content"
                                   title="tab.control.label">
                        <netuix:meta name="helpid" content="J2EE.ejb.ejbcontrol.title;j2ee"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portlet definitionLabel="EJBApplicationControlPortlet">
                                <netuix:strutsContent module="/j2ee" action="EJBControl" refreshAction="EJBControl"/>
                              </netuix:portlet>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the testing tab -->
                      <netuix:page markupName="page" definitionLabel="EJBApplicationTestingPage"
                                   presentationClass="page-content"
                                   title="tab.testing.label">
                        <netuix:meta name="helpid" content="J2EE.ejb.ejbtesting.title;j2ee"/>
                        <netuix:content>
                          <netuix:layout type="no" markupName="NoLayout">
                            <netuix:placeholder usingFlow="false" markupName="ph1">
                              <netuix:portletInstance markupType="Portlet" instanceLabel="EJBApplicationTestingPortlet"
                                                      contentUri="/PortalConfig/ejb/EJBTesting.portlet"/>
                            </netuix:placeholder>
                          </netuix:layout>
                        </netuix:content>
                      </netuix:page>
                      <!-- Definition for the monitoring tab -->
                      <netuix:book markupName="book" definitionLabel="EJBApplicationMonitorPage"
                                   title="tab.monitoring.label">
                        <netuix:singleLevelMenu markupType="Menu" markupName="singleLevelMenu" presentationClass="menu-level1"/>
                        <netuix:content>
                          <!-- Definition for the ejb stateless tab -->
                          <netuix:page markupName="page" definitionLabel="EJBApplicationMonitorEJBStatelessPage"
                                       presentationClass="page-content"
                                       title="tab.ejbstateless.label">
                            <netuix:meta name="helpid" content="J2EE.app.deploymentsmonitorejbstateless.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="EJBApplicationMonitorEJBStatelessPortlet">
                                    <netuix:strutsContent module="/j2ee" action="EJBMonitorRunningStateless"
                                                          refreshAction="EJBMonitorRunningStateless"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
                          </netuix:page>
                          <!-- Definition for the ejb stateful tab -->
                          <netuix:page markupName="page" definitionLabel="EJBApplicationMonitorEJBStatefulPage"
                                       presentationClass="page-content"
                                       title="tab.ejbstateful.label">
                            <netuix:meta name="helpid" content="J2EE.app.deploymentsmonitorejbstateful.title;j2ee"/>
                            <netuix:content>
                              <netuix:layout type="no" markupName="NoLayout">
                                <netuix:placeholder usingFlow="false" markupName="ph1">
                                  <netuix:portlet definitionLabel="EJBApplicationMonitorEJBStatefulPortlet">
                                    <netuix:strutsContent module="/j2ee" action="EJBMonitorRunningStateful"
                                                          refreshAction="EJBMonitorRunningStateful"/>
                                  </netuix:portlet>
                                </netuix:placeholder>
                              </netuix:layout>
                            </netuix:content>
